# Create .gitignore
cat > .gitignore << 'EOF'
# Environment and API Keys
.env
*.env
.env.local
.env.production

# Python
__pycache__/
*.py[cod]
*$py.class
*.so
.Python
build/
develop-eggs/
dist/
downloads/
eggs/
.eggs/
lib/
lib64/
parts/
sdist/
var/
wheels/
*.egg-info/
.installed.cfg
*.egg
MANIFEST

# Virtual Environment
nfl/
venv/
env/
ENV/

# Data Files (keep models but not raw data)
data/*.csv
data/*.json
data/*.parquet
data/*.xlsx
data/raw/
data/processed/
!data/.gitkeep
!data/README.md

# Jupyter Notebooks
.ipynb_checkpoints

# IDE
.vscode/
.idea/
*.swp
*.swo
*~

# OS
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Logs
*.log
logs/

# Model outputs (optional - you might want to track these)
models/*.pkl
models/*.joblib
models/*.h5

# Temporary files
tmp/
temp/
*.tmp

# Coverage reports
htmlcov/
.coverage
.coverage.*
coverage.xml

# pytest
.pytest_cache/
EOF